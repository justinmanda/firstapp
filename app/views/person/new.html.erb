<h1>Person#new</h1>
 <% form_for :person, @person, :url => { :action => "create" } do |f| %>

<p> <b>First Name</b><br /> <%= f.text_field :first_name, :helpText => 'First Name'%> </p> 
<p> <b>Last Name</b><br /> <%= f.text_field :last_name, :helpText => 'Last Name' %> </p> 
<p> <b>DOB </b><br /> <%= f.text_field :dob, :helpText => 'Date of Birth' %> </p> 
 

<p> <%= f.submit "create person" %> </p> 
<% end %>
