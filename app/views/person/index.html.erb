<table>
<tr><th>ID</th><th>First Name</th><th>Last Name</th><th>DOB</th><th>Created At</th><th>Updated At</th><td></th><td></th></tr>
<% @people.each do |person|%>
  <tr><td><%= person.id %></td>
      <td><%= person.first_name %></td>
      <td><%= person.last_name %></td>
      <td><%= person.dob.strftime("%d %b %Y") rescue " " %></td>
			<td><%= person.created_at.strftime("%d %b %Y") %></td>
			<td><%= person.updated_at.strftime("%d %b %Y") %></td>
      <td><%= person.updated_at.strftime("%d %b %Y") %></td>
      <td><%= link_to "Edit",  :controller => "person", :action => "edit" , :id => person.id %><td>
      <td><%= link_to "Delete",  :controller => "person", :action => "delete" , :id => person.id %><td>
  </tr>
<% end%>
</table>

<p><%= link_to "New Person", :controller => "person" , :action => "new"  %></p>
